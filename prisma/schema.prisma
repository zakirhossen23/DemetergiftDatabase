generator client {
    provider        = "prisma-client-js"
    previewFeatures = ["dataProxy"]
}

datasource db {
    provider = "postgresql"
    url      = env("DATABASE_URL")
}

model Event {
    id          Int    @id @default(autoincrement())
    title       String
    description String
    endDate     String
    Goal        Int
    logolink    String


    @@map(name: "events")
}

model Nftcryptopunk {
    id          Int    @id @default(autoincrement())
    eventid     Int
    name        String
    description String
    Bidprice    Int
    price       Int
    type        String
    image       String


    @@map(name: "nftcryptopunks")
}
